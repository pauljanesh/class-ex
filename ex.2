class Euler {
  static void euler1() {
    int s=0;
    for(int i=1;i<1000;i++)
      if(i%3==0||i%5==0) s+=i;
    System.out.println(s);
  }

  static void eulerCustom(int a,int b,int n){
    int s=0;
    for(int i=1;i<n;i++)
      if(i%a==0||i%b==0) s+=i;
    System.out.println(s);
  }

  public static void main(String[] args){
    euler1();           // Euler Q1
    eulerCustom(3,5,20); // Custom test
  }
}
