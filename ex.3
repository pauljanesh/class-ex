
// 1: takes two numbers and a list
function listEuler1(a, b, list) {
  let s = 0;
  for (let x of list) {
    if (x % a === 0 || x % b === 0) {
      s += x;
    }
  }
  return s;
}

// 2: takes two lists
function listEuler2(aList, list) {
  let s = 0;
  for (let x of list) {
    for (let a of aList) {
      if (x % a === 0) {
        s += x;
        break; // avoid counting same number multiple times
      }
    }
  }
  return s;
}

// 3: same logic, different parameter naming
function listEuler3(aList, list) {
  let s = 0;
  for (let x of list) {
    for (let a of aList) {
      if (x % a === 0) {
        s += x;
        break;
      }
    }
  }
  return s;
}

// Main
const l = [1, 2, 3, 4, 5, 6, 7, 9, 10, 10, 10];

console.log(listEuler1(2, 3, l));              // 66
console.log(listEuler2([2, 3], l));            // 66
console.log(listEuler3([2, 3, 5], l));         // 87
