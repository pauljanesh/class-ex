import java.util.Arrays;
import java.util.List;

class EulerListsShort {
    static int listEuler1(int a,int b,List<Integer> l){
        int s=0;
        for(int x:l) if(x%a==0||x%b==0) s+=x;
        return s;
    }

    static int listEuler2(List<Integer> aList,List<Integer> l){
        int s=0;
        for(int x:l) for(int a:aList) if(x%a==0){s+=x; break;}
        return s;
    }

    static int listEuler3(List<Integer> aList,List<Integer> l){
        int s=0;
        for(int x:l) for(int a:aList) if(x%a==0){s+=x; break;}
        return s;
    }

    public static void main(String[] args){
        List<Integer> l = Arrays.asList(1,2,3,4,5,6,7,9,10,10,10);
        System.out.println(listEuler1(2,3,l));       // 55
        System.out.println(listEuler2(Arrays.asList(2,3),l));   // 55
        System.out.println(listEuler3(Arrays.asList(2,3,5),l)); // 75
    }
}
